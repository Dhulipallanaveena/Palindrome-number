def checkPalindrome(num):
    m=num
    sum=0
    rem=0
    while m>0:
        rem=m%10
        sum=sum*10+rem
        m=m//10
    if sum==num:
        return True
    else:
        return False
		
num = int(input())
isPalindrome = checkPalindrome(num)
if(isPalindrome):
	print('true')
else:
	print('false')
